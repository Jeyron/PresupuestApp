        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Navegacion"
            android:label="PresupuestApp"
            android:theme="@style/AppTheme" />
        <activity android:name=".Reporte_detalle_egresos" />
        <activity android:name=".Reporte_balance_categoria_egreso" />
        <activity android:name=".Reporte_porcentaje_egresos_categoria" />
